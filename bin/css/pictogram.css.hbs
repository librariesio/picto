.pictogram {
  width:40px;
  height:40px;
  background: rgba(200,200,200,0.1) no-repeat;
  background-size: 100%;
  background-position: center center;
}
.pictogram-lg {
  width:60px;
  height:60px;
}
{{#each this}}
.pictogram-{{cssIdentifier}} {
  background-image: url({{url}});
  background-color: transparent;
}
{{/each}}
